  },
  "homepage": "https://github.com/gcba-iris/iris#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "chokidar": "^1.6.1",
    "cli-cursor": "^2.1.0",
    "decache": "^4.1.0",
    "fastparallel": "^2.3.0",
